  },
  "homepage": "https://github.com/ArtskydJ/music-room-site",
  "dependencies": {
    "abstract-state-router": "^5.11.0",
    "drag-drop": "^2.0.0",
    "just-login-core": "^2.0.1",
    "just-login-session-state": "^2.0.1",
    "level": "^0.18.0",
    "level-spaces": "^1.1.1",
    "playlist-combinator": "0.0.5",
    "ractive": "^0.7.3",
    "ractive-state-router": "^6.0.0",
    "socket.io": "^1.3.4",
